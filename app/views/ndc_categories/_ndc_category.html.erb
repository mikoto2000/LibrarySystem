<div id="<%= dom_id ndc_category %>">
  <table class="table table-hover">
    <tr>
      <td>
        <strong>Id:</strong>
      </td>
      <td>
        <%= ndc_category.id %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= NdcCategory.human_attribute_name(:name) %>
        </strong>
      </td>
      <td>
        <%= ndc_category.name %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= NdcCategory.human_attribute_name(:number) %>
        </strong>
      </td>
      <td>
        <%= ndc_category.number %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= NdcCategory.human_attribute_name(:created_at) %>
        </strong>
      </td>
      <td>
        <%= ndc_category.created_at && l(ndc_category.created_at) %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= NdcCategory.human_attribute_name(:updated_at) %>
        </strong>
      </td>
      <td>
        <%= ndc_category.updated_at && l(ndc_category.updated_at) %>
      </td>
    </tr>
  </table>
</div>
